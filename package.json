{
  "name": "js-component-seed",
  "version": "1.0.2",
  "description": "Start point for a javascript package",
  "main": "src/index.js",
  "scripts": {
    "prebuild": "rimraf build demo/node_modules/js-component-seed",
    "dev:copy-files": "cross-env NODE_ENV=development babel-node ./script/copy-files.js",
    "dev:es2015": "cross-env NODE_ENV=development babel ./src/ -d ./demo/node_modules/js-component-seed/ --ignore *.test.js",
    "dev:es2015modules": "cross-env NODE_ENV=development BABEL_ENV=modules babel ./src/index.js -o ./demo/node_modules/js-component-seed/index.es.js",
    "dev:es": "cross-env NODE_ENV=development BABEL_ENV=es babel ./src/ -d ./demo/node_modules/js-component-seed/es/ --ignore *.test.js",
    "dev": "yarn dev:es2015 && yarn dev:es2015modules && yarn dev:es",
    "start": "yarn prebuild && yarn dev:copy-files && nodemon --exec yarn dev",
    "build:copy-files": "cross-env NODE_ENV=production babel-node ./script/copy-files.js",
    "build:es2015": "cross-env NODE_ENV=production babel ./src/ -d ./build/ --ignore *.test.js",
    "build:es2015modules": "cross-env NODE_ENV=production BABEL_ENV=modules babel ./src/index.js -o ./build/index.es.js",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel ./src/ -d ./build/es/ --ignore *.test.js",
    "build": "yarn build:es2015 && yarn build:es2015modules && yarn build:es && yarn build:copy-files",
    "doc": "jsdoc src -r",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "preversion": "yarn test",
    "version": "yarn build && npm publish build"
  },
  "repository": "git@github.com:marcosun/js-component-seed.git",
  "author": "marcosun <marcoinbristol@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.2.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-babel": "^5.2.0",
    "eslint-plugin-import": "^2.14.0",
    "fs-extra": "^7.0.0",
    "jest": "^23.4.1",
    "nodemon": "^1.18.3",
    "rimraf": "^2.6.2"
  }
}
